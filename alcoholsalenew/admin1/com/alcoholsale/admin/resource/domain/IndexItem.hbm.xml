<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.alcoholsale.admin.resource.domain.IndexItem" table="T_INDEXITEM" schema="ALCOHOLSALE" >
        <id name="IndexItemid" type="java.lang.Integer">
            <column name="INDEXITEMID" precision="22" scale="0" />
            <generator class="sequence" >
            	<param name="sequence">seq_indexitemid</param>
            </generator>
        </id>
        <property name="title" type="java.lang.String" >
			<column name="title" length="64"></column>
		</property>
        <property name="url" type="java.lang.String" >
			<column name="url" length="64"></column>
		</property>
  		<property name="sort" type="java.lang.Integer">
		<column name="sort" length="20"></column>
	</property>
    </class>
</hibernate-mapping>
